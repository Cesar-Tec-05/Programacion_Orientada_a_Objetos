@startuml Geometria

abstract class Figuras_back {
    - float arista
    - float arista2
    - float arista3
    + getArea(): float
    + getPerimetro(): float
    + setArista(float): void
    + getArista(): float
    + setArista2(float): void
    + getArista2(): float
    + setArista3(float): void
    + getArista3(): float
    + clearAttributes(): void
}

class Circulo_back {
    # float PI
    + getArea(): void
